= javascript_include_tag "application", "data-turbolinks-track" => true
-if notice.present?
  :javascript
    noty({text: '#{notice}', timeout: 3000, layout: 'bottomLeft', type: 'success'});
-if alert.present?
  :javascript
    noty({text: '#{alert}', timeout: 3000, layout: 'bottomLeft', type: 'error'});
